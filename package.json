{
  "name": "express-backpack-babel-starting-boilerplate",
  "version": "2.0.1",
  "description": "Starting point for a new Node.js Api",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "production": "NODE_ENV=production node ./build/index.js",
    "build": "rm -rf build && mkdir build && babel -d ./build ./src -s",
    "start": "NODE_ENV=development backpack",
    "debug": "NODE_ENV=development nodemon --exec 'npm run lint && npm run build && node --debug ./build/index.js'",
    "lint": "eslint -c .eslintrc src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hqro/Express-Backpack-Babel-ES7-Api-Boilerplate"
  },
  "keywords": [
    "backpack-core",
    "express",
    "babel",
    "es7"
  ],
  "author": "Guillaume L. <glarivie@student.42.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hqro/Express-Backpack-Babel-ES7-Api-Boilerplate/issues"
  },
  "homepage": "https://github.com/hqro/Express-Backpack-Babel-ES7-Api-Boilerplate#readme",
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "helmet": "^3.4.0",
    "is_js": "^0.8.0",
    "joi": "^9.0.4",
    "lodash": "^4.14.1",
    "mongoose": "^4.5.7",
    "mongoose-gen": "^2.1.1",
    "mongoose-paginate": "^5.0.0",
    "query-string": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-polyfill": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "backpack-core": "0.0.7",
    "bunyan": "^1.8.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-import": "^1.12.0",
    "morgan": "^1.7.0",
    "nodemon": "^1.10.0"
  },
  "optionalDependencies": {
    "fsevents": "1.0.15"
  }
}
